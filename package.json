  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "tap-nyan": "^1.1.0",
    "xo": "^0.17.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "ava": {
