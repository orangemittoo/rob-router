{
  "name": "rob-router",
  "version": "0.0.2",
  "description": "rob-router is router for web browser like Ruby on Rails",
  "homepage": "https://github.com/orangemittoo/rob-router",
  "main": "dest/rob-router.js",
  "scripts": {
    "test": "xo | ava --tap | tap-nyan",
