{
  "name": "rob-router",
  "version": "1.0.5",
  "description": "rob-router is router for web browser like Ruby on Rails",
  "homepage": "https://github.com/orangemittoo/rob-router",
  "main": "dest/rob-router.js",
  "scripts": {
    "test": "ava",
    "build": "babel src/rob-router.js -o dest/rob-router.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/orangemittoo/rob-router"
  },
  "keywords": [
    "routes",
    "router",
    "rails",
    "browser",
    "url",
    "dispatcher"
  ],
  "author": "orangemittoo",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "ava": "^3.9.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "edge": "17",
            "firefox": "60",
            "chrome": "67",
            "safari": "11.1"
          },
          "useBuiltIns": "entry",
          "corejs": "3.6.4"
        }
      ]
    ]
  },
  "ava": {
    "babel": true,
    "require": [
      "@babel/register"
    ]
  },
  "dependencies": {
    "core-js": "^3.6.5"
  }
}
